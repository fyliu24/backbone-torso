{
  "tags": {
    "allowUnknownTags": false
  },
  "plugins": ["plugins/markdown"],
  "opts": {
    "template": "node_modules/docdash",
    "encoding": "utf8",
    "recurse": true,
    "verbose": false
  },
  "markdown": {
    "parser": "gfm",
    "hardwrap": true,
    "idInHeadings": true
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "default": {
      "outputSourceFiles": true,
      "includeDate": false,
      "useLongnameInNav": true
    }
  },
  "docdash": {
    "static": true,
    "sort": true,
    "disqus": "",
    "openGraph": {
      "title": "backbone-Torso",
      "type": "website",
      "image": "https://vecnatechnologies.github.io/backbone-torso/images/black_website_logo.svg",
      "site_name": "backbone-Torso",
      "url": "https://vecnatechnologies.github.io/backbone-torso/"
    },
    "meta": {
      "title": "backbone-Torso",
      "description": "A holistic approach to Backbone applications",
      "keyword": "backbone"
    },
    "search": true,
    "collapse": true,
    "typedefs": true,
    "removeQuotes": "none",
    "menu": {
      "Github repo":{
        "href": "https://github.com/vecnatechnologies/backbone-torso",
        "target": "_blank",
        "class": "menu-item",
        "id": "repository"
      },
      "Documentation Index": {
        "href": "../README.md",
        "class": "menu-item",
        "id": "all_docs_index_link"
      },
      "Annotated Source": {
        "href": "../ANNOTATED.md",
        "class": "menu-item",
        "id": "annotated_link"
      },
      "Interactive Docs": {
        "href": "../interactive/index.html",
        "class": "menu-item",
        "id": "interactive_docs_link"
      }
    }
  }
}